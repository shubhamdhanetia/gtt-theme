<div aria-hidden="true" class="modal fade" id="modal-error">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" data-dismiss="modal" type="button">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4>{{ 'popup.error.title' | t }}</h4>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary themed" data-dismiss="modal" type="button">{{ 'popup.error.ok' | t }}</button>
            </div>
        </div>
    </div>
</div>
